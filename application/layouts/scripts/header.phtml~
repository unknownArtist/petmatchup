  function initialize() {
    var latlng = new google.maps.LatLng(-73.0464, 40.8131);
    var myOptions = {
      zoom: 8,
      center: latlng,
      mapTypeId: google.maps.MapTypeId.ROADMAP
    };
    var map = new google.maps.Map(document.getElementById("map_canvass"),
        myOptions);
  }

